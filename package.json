    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^29.2.2",
    "@types/react": "^16.14.43",
    "@types/react-dom": "^16.9.19",
    "auto-changelog": "^2.4.0",
    "husky": "^4.2.5",
    "jest": "^29.3.0",
